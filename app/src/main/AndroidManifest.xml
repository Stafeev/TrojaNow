        <activity android:name=".LoginActivity" >
        </activity>
         <activity android:name=".MainPostFeedActivity" >
        </activity>
        
        <activity
            android:name=".SignUpActivity"
            android:label="@string/title_activity_sign_up" >
        </activity>
        <activity
            android:name=".NewPostActivityActivity" >
        </activity>
    </application>

</manifest>
